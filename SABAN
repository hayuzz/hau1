-- Keep loading until Joebiden flag is set
repeat
    wait()
    spawn(function()
        loadstring(game:HttpGet("https://nousigi.com/loader.lua"))()
    end)
    wait(20)
until getgenv().Joebiden
spawn(function()
    -- ‚öôÔ∏è Setup
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Merchant = workspace:WaitForChild("Merchant")
    local MerchantData = require(ReplicatedStorage.Datas.MerchantData)
    local BuyRemote = ReplicatedStorage.Packages.Net:FindFirstChild("RF/MerchantService/Buy")
    local CountdownLabel = Merchant.Overhead.BillboardGui.Countdown

    -- C·∫•u h√¨nh
    local CHECK_INTERVAL = 1     -- Ki·ªÉm tra countdown m·ªói gi√¢y
    local START_SPAM_TIME = 5    -- Khi c√≤n <= 2 gi√¢y th√¨ spam
    local SPAM_DURATION = 10      -- Spam li√™n t·ª•c 5 gi√¢y
    local MERCHANT_ID = "2298b248-44dd-4a34-bbfe-e1174e8ae1b4"

    -- üïí H√†m l·∫•y s·ªë gi√¢y c√≤n l·∫°i trong countdown
    local function getRestockSeconds()
        if not CountdownLabel or not CountdownLabel.Text then return 9999 end
        local text = CountdownLabel.Text
        local m, s = text:match("Restocking in (%d+)m (%d+)s")
        if m and s then return tonumber(m)*60 + tonumber(s) end
        local sec = text:match("Restocking in (%d+)s")
        if sec then return tonumber(sec) end
        return 0
    end

    -- üîÅ Loop ch√≠nh: check th·ªùi gian & spam khi c·∫ßn
    while task.wait(CHECK_INTERVAL) do
        local remaining = getRestockSeconds()

        if remaining <= START_SPAM_TIME then
            print(string.format("[‚ö°] Restock c√≤n %.1fs ‚Üí b·∫Øt ƒë·∫ßu spam 5s Secret+!", remaining))
            local t0 = tick()

            repeat
                for i, v in MerchantData.Brainrots do
                    if i >= 7 then -- Secret tr·ªü l√™n
                        task.spawn(function()
                            pcall(function()
                                BuyRemote:InvokeServer(MERCHANT_ID, i)
                            end)
                        end)
                    end
                end
                task.wait()
            until tick() - t0 > SPAM_DURATION

            print("[‚èπÔ∏è] D·ª´ng spam (ƒë√£ h·∫øt 5s auto-buy)")
        else
            print(string.format("[‚åõ] C√≤n %.1fs m·ªõi restock", remaining))
        end
    end
end)
